{
  "name": "travel-mvp",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "pages:dev": "npm run build && wrangler pages dev ./dist",
    "pages:deploy": "npm run build && wrangler pages deploy ./dist",
    "test": "vitest run -c tests/vitest.config.ts",
    "test:watch": "vitest -c tests/vitest.config.ts",
    "test:api": "vitest run -c tests/vitest.config.ts tests/api/",
    "test:api:noai": "cross-env TEST_SKIP_AI=true vitest run -c tests/vitest.config.ts tests/api/",
    "test:ai": "vitest run -c tests/vitest.config.ts tests/api/assistant.test.ts",
    "test:integration": "vitest run -c tests/vitest.config.ts tests/api/integration.test.ts"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.80",
    "@react-google-maps/api": "^2.20.7",
    "@react-oauth/google": "^0.12.2",
    "@types/leaflet": "^1.9.21",
    "@types/react-beautiful-dnd": "^13.1.8",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "idb": "^8.0.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.564.0",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.5.4",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.1.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "daisyui": "^5.4.7",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^28.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.0.0"
  }
}
